{
  "name": "stampy",
  "version": "0.4.0",
  "description": "The new and improved stampy",
  "repository": {
    "type": "git",
    "url": "git@github.com:bigdatr/stampy.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "babel src -d lib --watch",
    "build": "rm -rf lib && babel src --out-dir lib",
    "prepublish": "babel src --out-dir lib",
    "lint": "eslint src/**/*.jsx src/**/*.js",
    "lint-fix": "eslint src/**/*.jsx src/**/*.js --fix",
    "ava": "ava",
    "test": "nyc --report-dir=./coverage ava && yarn run check-coverage",
    "test-all": "yarn run lint && yarn run flow && yarn run test",
    "check-coverage": "nyc check-coverage --branches 0.0  --functions 0.0 --lines 0",
    "docs": "jsdoc -c jsdoc-config.json",
    "publish-docs": "jsdoc -c jsdoc-config.json && gh-pages --dist docs --no-push",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "devDependencies": {
    "app-module-path": "^2.0.0",
    "autoprefixer": "^6.5.2",
    "ava": "^0.16.0",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-blueflag": "^0.0.1",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.5.0",
    "enzyme": "^2.5.1",
    "eslint": "^2.7.0",
    "eslint-config-blueflag": "^0.2.0",
    "flow-bin": "^0.33.0",
    "gh-pages": "^0.11.0",
    "git-url-parse": "^6.0.1",
    "jsdoc": "^3.4.2",
    "jsdoc-babel": "^0.2.1",
    "jsdonk": "0.1.7",
    "node-sass": "^3.11.2",
    "nyc": "^6.6.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1"
  },
  "main": "lib/index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "presets": [
        "blueflag"
      ]
    },
    "files": [
      "src/**/*-test.js"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/**/*-test.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "require": [
      "babel-register"
    ]
  }
}
